{
  "name": "vue-ssr-skeleton",
  "description": "A Vue.js skeleton including SSR, vue-meta and tests. Based on vue-hackernews-2.0 by Evan You.",
  "author": "Camilo Delvasto <camilodelvasto@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server",
    "start": "cross-env NODE_ENV=production node server",
    "postinstall": "npm run build",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "serve": "node server.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "test": "npm run test:unit",
    "test:e2e": "rimraf src/tests/e2e/reports && cross-env NODE_ENV=test node src/tests/e2e/runner.js",
    "test:unit": "rimraf src/tests/unit/coverage && cross-env NODE_ENV=test karma --timeout 20000 start src/tests/unit/karma.conf.js --single-run --display-error-details"
  },
  "engines": {
    "node": "9.11.1",
    "npm": "5.6.0"
  },
  "dependencies": {
    "array-to-tree": "^3.3.0",
    "axios": "^0.18.0",
    "bulma": "^0.7.1",
    "compression": "^1.7.1",
    "dotenv": "^5.0.1",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "flickity": "^2.1.1",
    "intersection-observer": "^0.5.0",
    "javascript-time-ago": "^1.0.30",
    "json-server": "^0.12.2",
    "lodash": "^4.17.10",
    "lru-cache": "^4.1.1",
    "plyr": "^3.3.22",
    "route-cache": "0.4.3",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.17",
    "vue-avatar": "^2.1.4",
    "vue-axios": "^2.1.1",
    "vue-croppa": "^1.3.7",
    "vue-meta": "^1.5.0",
    "vue-no-ssr": "^0.2.2",
    "vue-plyr": "github:camilodelvasto/vue-plyr",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.11.0",
    "vue-select": "^2.4.0",
    "vue-server-renderer": "^2.5.16",
    "vue-wysiwyg-2": "^1.7.92",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "8",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chokidar": "^1.7.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "karma": "^2.0.2",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.4",
    "mocha": "^5.1.1",
    "node-sass": "^4.9.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "sass-resources-loader": "^1.3.3",
    "sinon": "^6.1.3",
    "sinon-chai": "^2.12.0",
    "style-loader": "^0.21.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^2.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "vue-loader": "^15.0.9",
    "vue-template-compiler": "^2.5.16",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.6.0"
  }
}
